class Basicoperator:
    def s(self,n):
        m=0
        i=1
        for i in range(1,n+1):
            m=i+m
        else:
            print(m)
    def j(self,n):
        m=1
        i=1
        for i in range(1,n+1):
            m=i*m
        else:
            print(m)
a=Basicoperator()
a.s(100)
a.j(5)
