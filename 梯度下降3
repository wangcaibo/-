import random
x = [(2.014,3),(1.600, 3),(2.400,3),(1.416,2),(3.000,4)]
y = [400, 330, 369, 232, 540]
m=len(x)
a=0.001
t=0.01
a0=1
a1=1
a2=1
a00=100
a11=100
a22=100
b=[0,1,2,3] 
while(a00-a0>a or a11-a1>a or a22-a2>a):    
    J0=0
    J1=0
    J2=0
    i=random.choice(b)
    h=a0+a1*x[i][0]+a2*x[i][1]
    J0=J0+h-y[i]
    J1=J1+(h-y[i])*x[i][0]
    J2=J2+(h-y[i])*x[i][1]
    a00=a0
    a11=a1
    a22=a2
    a0=a0-t*J0/m
    a1=a1-t*J1/m
    a2=a2-t*J2/m
print(a0,a1,a2)
